@model ListViewModel

<table class="table">
    <tr>
        <th>Url</th>
        <th>ShortUrl</th>
        <th>Date</th>
        <th></th>
    </tr>
    @foreach (var item in Model.UrlMappingEntries)
    {
        <tr>
            <td>@item.Url</td>
            <td>@($"https://{Model.Host}/{item.Id}")</td>
            <td>@item.Created</td>
            <td>
                <a class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>